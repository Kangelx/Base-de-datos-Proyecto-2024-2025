<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.api.reto.models.PersonalEntity">
        <table name="personal" schema="tic_incidencias" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="dni">
                <column name="dni" nullable="false" length="9"/>
            </basic>
            <basic name="nombre">
                <column name="nombre" length="25"/>
            </basic>
            <basic name="apellido1">
                <column name="apellido1" length="25"/>
            </basic>
            <basic name="apellido2">
                <column name="apellido2" nullable="false" length="25"/>
            </basic>
            <basic name="direccion">
                <column name="direccion" nullable="false" length="80"/>
            </basic>
            <basic name="localidad">
                <column name="localidad" nullable="false" length="25"/>
            </basic>
            <basic name="cp">
                <column name="cp" nullable="false" length="5"/>
            </basic>
            <basic name="tlf">
                <column name="tlf" nullable="false" length="9"/>
            </basic>
            <basic name="activo">
                <column name="activo" nullable="false" precision="-1"/>
            </basic>
            <basic name="departamentoId">
                <column name="departamento_id" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>

<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.api.reto.models.DepartamentosEntity">
        <table name="departamentos" schema="tic_incidencias" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="cod">
                <column name="cod" length="6"/>
            </basic>
            <basic name="nombre">
                <column name="nombre" length="45"/>
            </basic>
            <basic name="activo">
                <column name="activo" precision="-1"/>
            </basic>
            <basic name="jefedepId">
                <column name="jefedep_id" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>

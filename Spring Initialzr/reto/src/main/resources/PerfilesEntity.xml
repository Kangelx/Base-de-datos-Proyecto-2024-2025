<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.api.reto.models.PerfilesEntity">
        <table name="perfiles" schema="tic_incidencias" catalog=""/>
        <attributes>
            <id name="personalId">
                <column name="personal_id" precision="-1"/>
            </id>
            <basic name="dominio">
                <column name="dominio" nullable="false" length="15"/>
            </basic>
            <basic name="educantabria">
                <column name="educantabria" nullable="false" length="50"/>
            </basic>
            <basic name="password">
                <column name="password" nullable="false" length="32"/>
            </basic>
            <basic name="perfil">
                <column name="perfil" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>

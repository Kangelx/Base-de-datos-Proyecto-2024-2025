<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.api.reto.models.EquiposEntity">
        <table name="equipos" schema="tic_incidencias" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="tipoEquipo">
                <column name="tipo_equipo" precision="-1"/>
            </basic>
            <basic name="fechaAdquisicion">
                <column name="fecha_adquisicion" nullable="false" precision="-1"/>
            </basic>
            <basic name="etiqueta">
                <column name="etiqueta" length="8"/>
            </basic>
            <basic name="marca">
                <column name="marca" length="20"/>
            </basic>
            <basic name="modelo">
                <column name="modelo" nullable="false" length="45"/>
            </basic>
            <basic name="descripcion">
                <column name="descripcion" nullable="false"/>
            </basic>
            <basic name="baja">
                <column name="baja" nullable="false" precision="-1"/>
            </basic>
            <basic name="aulaNum">
                <column name="aula_num" precision="-1"/>
            </basic>
            <basic name="puesto">
                <column name="puesto" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
